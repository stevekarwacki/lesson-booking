{
  "name": "lesson-booking",
  "version": "1.0.0",
  "description": "A lesson booking app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "cd frontend && npm install && npm run build",
    "dev": "nodemon server.js",
    "redis": "redis-server --daemonize yes",
    "deploy": "npm run build && npm start",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "node --test tests/*.test.js",
    "test:frontend": "cd frontend && npm run test:run",
    "test:watch": "npm run test:backend:watch & npm run test:frontend:watch",
    "test:backend:watch": "node --test --watch tests/*.test.js",
    "test:frontend:watch": "cd frontend && npm run test",
    "test:integration": "node tests/integration-test.js",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Steve Karwacki",
  "license": "MIT",
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@casl/vue": "^2.2.2",
    "@google-cloud/local-auth": "^3.0.1",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^154.0.0",
    "handlebars": "^4.7.8",
    "ical-generator": "^9.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "pg": "^8.11.3",
    "pinia": "^3.0.2",
    "sequelize": "^6.35.1",
    "sqlite3": "^5.1.7",
    "stripe": "^18.1.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "nodemon": "^3.0.3"
  }
}
